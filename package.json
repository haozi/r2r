{
  "name": "r2r",
  "version": "0.0.0",
  "description": "",
  "main": "lib/index.js",
  "scripts": {
    "dev": "npm run debug | npm run watch",
    "test": "ava test/",
    "test-cov": "istanbul cover ./node_modules/mocha/bin/_mocha -- -t 50000 --recursive  -R spec test/",
    "copy": "rm -f npm-debug.log.* && rm -rf lib && cp -r src lib",
    "build": "npm run lint && npm run copy && babel --presets es2015,es2016,es2017 --plugins transform-runtime src/ --out-dir lib/ --source-maps inline",
    "watch-build": "npm run build -- --watch",
    "watch": "npm run watch-build",
    "lint": "standard 'src/**/**.js' --fix",
    "debug": "sudo devtool test/express.js --watch >/dev/null 2>&1",
    "prepublish": "npm run lint && npm run build"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-es2016": "^6.22.0",
    "babel-preset-es2017": "^6.22.0",
    "express": "^4.14.1",
    "standard": "^8.6.0"
  },
  "dependencies": {
    "jsonuri": "^1.5.13",
    "lodash.merge": "^4.6.0",
    "node-lancer": "^1.0.14"
  }
}
